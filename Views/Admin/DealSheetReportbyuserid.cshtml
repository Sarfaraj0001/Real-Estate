
@using ODHDEVELOPERS.Models;
@using System;
@{
    ViewBag.Title = "AgentWelcomeLetter";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script>
    $(function () {
        $('[name=Userid]').autocomplete({
            source: function (request, response) {
                debugger;
                $.ajax({
                    url: "/Branch/DealSheetReportbyuserid",
                    type: "Get",
                    datatype: "JSON",
                    data: { Userid: request.term, Recogniser: "Print" },
                    success: function (data) {
                        debugger;
                        response($.map(data, function (item) {
                            return { value: item.userid, label: item.customername }
                        }));
                    }
                });
            }
        });

        $("#btn").on("click", function (e) {

            // window.open("");

        });

    });

</script>


<div class="page-content">

    <div class="container">

        <div class="page-toolbar">

            <div class="page-toolbar-block">
                <div class="page-toolbar-title">Deal Sheet Report</div>
                <div class="page-toolbar-subtitle"></div>
            </div>



        </div>

        <div class="widget stacked">

            <div class="widget-header">
                <i class="icon-desktop"></i>
                <h3>Deal Sheet</h3>
            </div>
            <!-- /widget-header -->

            <div class="widget-content">
                <div class="shortcuts">
                    <form action="/Admin/DealSheetReportbyuserid" method="post" target="_blank">
                        <div class="row " id="hide">
                            <div class="col-md-2"></div>
                            <div class="col-md-8">
                                @* <label style="font-weight: bold">Enter the Deal Id :</label>*@
                                <input type="text" name="Userid" placeholder="Enter the Deal Id " class="form-control" />
                            </div>
                            <div class="col-md-2">                              
                                <button id="btn" type="submit" class="btn btn-primary btn-md" style="margin-top: 0px;">Print Deal Sheet</button>
                            </div>
                        </div>
                    </form>


                </div>
                <!-- /shortcuts -->

            </div>
            <!-- /widget-content -->

        </div>
    </div>

</div>





