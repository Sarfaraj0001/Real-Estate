@model ODHDEVELOPERS.Models.Ranktab
@using ODHDEVELOPERS.Models;
@{
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);   
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "CreateRank";
}



<div class="page-content">

                <div class="container">

<div class="page-toolbar">
                        
                        <div class="page-toolbar-block">
                            <div class="page-toolbar-title">Create @mr.rankname</div>
                            <div class="page-toolbar-subtitle"></div>
                        </div>
                        
                        <div class="page-toolbar-block pull-right">
                            
                        </div>           
                        
                    </div> 

                    <div class="row" style="width:100%">
                        <div class="col-md-6" style="width:100%">
                            
                            <div class="block" style="width:100%">
                               <div class="block-content controls">  

                    
                      
                        
     @using (Html.BeginForm()) {
    

                                    
                   <div class="row-form">  
                                         
    <div class="col-md-9">
        <div class="input-group">
       <span class="input-group-addon"><label class="width">@mr.rankname Name</label> </span>  
        <input type="text" class="form-control"  name="RankName" id="rankname" required/></div>
                                   
   </div>
                        
         </div>
         
         <div class="row-form">  
                       <div class="col-md-9">
        <div class="input-group">
       <span class="input-group-addon"><label class="width">@mr.rankname Shortcut</label> </span>  
        <input type="text" class="form-control"  name="Rankshrtcut" id="rankshortcut" required/></div>
                                   
   </div>
</div>
          <div class="row-form">
                    <div class="col-md-9">
      <div class="input-group">
       <span class="input-group-addon"><label class="width">Self Joining</label></span>
        <input type="number" step="any" class="form-control"  name="Selfquota" id="selfquota" required />
         
        </div>
      </div>
               </div>
         
         
         <div class="row-form">
             
      <div class="col-md-9">
          <div class="input-group">
       <span class="input-group-addon"><label class="width">Team Quota</label></span>
          <input type="number" step="any" class="form-control" name="TeamQuota" id="teamquota" required/></div>
      </div>
                           </div>
         
            
                
                 <div class="page-toolbar-block pull-right">
                            <div class="widget-info widget-from">
         <button  type="submit" class="btn btn-primary pull-right"> Create @mr.rankname</button>   
                                </div>
                                             </div>
         
     <p style="color:maroon;"><b> @ViewBag.msg</b></p>
     }
                                </div>
                                
                            
                                         
                                    </div>
                            </div>
                                        
                   </div> </div> </div>