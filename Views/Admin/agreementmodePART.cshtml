@model IEnumerable<ODHDEVELOPERS.Models.appltab>
@using ODHDEVELOPERS.Models;
@{
    ViewBag.Title = "agreementmodePART";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);
    var a = 0;
}

<link rel="stylesheet" href="~/Content/jquery-ui.css" />
<script type="text/javascript">
    $(document).ready(function () {
        $("#newbondid").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/Branch/AutoCompleteBondId_Part",
                    type: "POST",
                    dataType: "json",
                    data: { term: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.name, value: item.newbondid };
                        }))

                    }
                })
            },
            messages: {
                noResults: "", results: ""
            }
        });
    })
</script>


<div class="page-content">
         <div>
            <div></div>
                <div class="container" id="divid1">
                    <div class="page-toolbar" id="2">
                        
                        <div class="page-toolbar-block">
                            <div class="page-toolbar-title">@mr.custname AGREEMENT TO SELL PROPERTY (PART)</div>
                            <div class="page-toolbar-subtitle"></div>
                        </div>
                        
                        <div class="page-toolbar-block pull-right">
                         
                        </div>           
                     
                    </div>
 
  @using (@Html.BeginForm())
  {
                   <div class="row" id="hide">
                        <div class="col-md-6" style="width:100%">
                            
                            <div class="block" >
                               
                                <div class="block-content controls" >
                                     <div class="row-form">
           <div class="input-group" style="margin-left:1%;margin-right:1%;">
                        <span class="input-group-addon">Enter @mr.custname Id</span>
                    <input type="text" id="newbondid" class="form-control" required name="customerid">
                         
                </div>   </div>
                   <div class="row-form" style="width:100%">
      <div class="col-md-9">
          <button id="submit" value="Search" class="btn btn-primary pull-right" type="submit" >View</button>
     </div>
              </div>



        </div>
        </div>
                             </div>
        </div>
  }
     
                    @if(Model!=null)
                    {                                                          
                        <table border="1" style="margin-left:1%; margin-right:1px; width:95%; margin-bottom:70px;">
   
        
                        
    <tr>
        <th style="width:5%;text-align:center;">
           <label id="sn"><b>S.N.</b></label>
            </th>
        <th style="width:10%;text-align:center;">
           <label id="planname"><b>Customer Name</b></label>
            </th>
        
        <th style="width:10%;text-align:center;">
           <label id="term_plan"><b>Customer Id</b></label>
        </th>
            
        <th style="width:10%;text-align:center;">
           <label id="mode"><b>Join Date</b></label>
        </th>
       <th style="width:10%;text-align:center;">
           <label id="mode1"><b>Print</b></label>
        </th>
    </tr>
           

    @foreach (var item in Model)
        {
            a = a + 1;
    <tr style="text-align:center;">
        <td> @a </td>
        <td>@item.name</td>
        <td>@item.newbondid</td>        
        <td>@item.formdate.ToString("dd/MM/yyyy")</td>
        <td> <a href="@Url.Action("printemiaggreementPART", "Customer", new {newid= item.newbondid})" target="_blank"><h5 style="color:#0f6db1;">Print Agreement</h5></a></td>

    </tr>
           } 

</table>
                    }

    </div>
    </div>
    </div>


