<script type="text/javascript">
    $(function () {
        (function () {
            var v = $('#category').attr('data-v');
            $('#category option').each(function () {
                var c = $(this), cv = c.val();
                if (cv == v) {
                    c.attr('selected', '');
                    return false;
                }
            });
        })();
    });
</script>