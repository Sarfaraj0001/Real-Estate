<script type="text/javascript">
    function sendmesg() {
        debugger;
        var memberid = $("#memberid").val();
        if (memberid != '') {
            $.ajax({
                url: "/Home/forgotpassword",
                type: "GET",
                dataType: "json",
                data: { userid: memberid },
                success: function (data) {
                    debugger;
                    for (var i = 0; i < data.length; i++) {
                        alert(data[i].sms);
                    }
                }
            })


        }
        else {
            alert('please enter userid!!');
        }
    }

</script>